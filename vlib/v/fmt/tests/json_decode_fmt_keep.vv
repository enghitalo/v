import x.json2 as json
import x.json2.decoder2

struct Request {
	a int
}

fn parse(s string) !Request {
	return decoder2.decode[Request](s)
}

fn parse2(s string) !Request {
	req := decoder2.decode[Request](s)!
	return req
}

fn main() {
	println(parse('{"a": 22} ')!)
	println(parse2('{"a": 22} ')!)
}
