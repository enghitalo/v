fn test_array_printing_strings() {
	// Test string array printing
	strings := ['hello', 'world', 'test']
	println(strings)
	
	// Test empty array
	empty := []string{}
	println(empty)
	
	// Test single element
	single := ['one']
	println(single)
}

fn test_array_printing_ints() {
	// Test int array printing
	nums := [1, 2, 3, 4, 5]
	println(nums)
	
	// Test empty int array
	empty := []int{}
	println(empty)
	
	// Test single element
	single := [42]
	println(single)
	
	// Test negative numbers
	negatives := [-10, -5, 0, 5, 10]
	println(negatives)
}
