// Test array struct field access in WASM backend
// This test demonstrates direct access to array struct fields

fn test_array_with_u16() {
	// Test with u16 array
	a := [u16(1), 2, 3]
	println('u16 array')
	println(a.data)
	println(a.len)
	println(a.cap)
	println(a.offset)
	println(a.element_size)
}

fn test_array_with_ints() {
	// Test with int array
	c := [100, 200, 300, 400]
	println('int array')
	println(c.data)
	println(c.len)
	println(c.cap)
	println(c.offset)
	println(c.element_size)
}

fn main() {
	test_array_with_u16()
	test_array_with_ints()
}
